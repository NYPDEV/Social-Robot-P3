<?xml version="1.0" encoding="utf-8"?>
<grammar xml:lang="en-GB" root="SGM_FUNC_StopReadNews" version="1.0" xmlns="http://www.w3.org/2001/06/grammar">
  <rule id="SGM_FUNC_StopReadNews" scope="public">
    <one-of>
      <item>stop reading news</item>
      <item>stop reading english news</item>
      <item>stop reading please</item>
      <item>stop reading now</item>
      <item>please stop reading</item>
      <item>please stop news reading</item>
      <item>stop english news</item>
    </one-of>
  </rule>
</grammar>