<grammar xml:lang="en-GB" root="SGM_FUNC_AskWhatDayIsToday" version="1.0" xmlns="http://www.w3.org/2001/06/grammar">
  <rule id="SGM_FUNC_AskWhatDayIsToday" scope="public">
